#STAMPA DEI CARATTERI NON PRESENTI IN UNA STRINGA 
.data
frase1: .asciiz "inserisci la stringa "
pangram: .asciiz "stringa pangram" 


stringa: .space 100

.text

.globl main
main:

la $a0,frase1
li $v0,4
syscall

la $a0,stringa
li $v0,8
li $a1,98
syscall

li $t3,97

#devo fare attenzione perchè la testa della stringa  stringa viene ricaricata di nuovo 

li $t7,0 #contatore 
char_non_presenti:
la $a0,stringa
move $t9,$a0

li $t4,0 #flag 


move $t9,$a0
ciclo_inserimento:
lbu $t2,($t9)
beq $t2,'\n',uscita
bgt $t3,122,confronta

beq $t2,$t3,ok
addi $t9,$t9,1
j ciclo_inserimento

ok:
li $t4,1

addi $t9,$t9,1
j ciclo_inserimento
uscita:
beq $t4,0,flag0


addi $t3,$t3,1
j char_non_presenti 
flag0:
addi $t7,$t7,1
move $a0,$t3
li $v0,11
syscall 
addi $t3,$t3,1
j char_non_presenti 
confronta:

beq $t7,0,pangrams
j exit 
pangrams:
la $a0,pangram
li $v0,4
syscall 

exit:

li $v0,10
syscall